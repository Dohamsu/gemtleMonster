{
  "name": "gemtlemonster",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=20.19.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,css}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,json,css}\"",
    "seed:facilities": "tsx src/data/seedFacilities.ts",
    "seed:alchemy": "tsx src/data/seedAlchemy.ts",
    "seed:monsters": "tsx src/data/seedMonsters.ts",
    "seed:skills": "tsx src/data/seedMonsterSkills.ts",
    "seed:equipment": "tsx src/data/seedEquipment.ts",
    "seed:all": "npm run seed:materials && npm run seed:facilities && npm run seed:alchemy && npm run seed:monsters && npm run seed:skills && npm run seed:equipment",
    "types:generate": "supabase gen types typescript --project-id zcqejdvtwubaodfpjvbm --schema public > src/types/database.types.ts",
    "types:local": "supabase gen types typescript --local --schema public > src/types/database.types.ts",
    "db:pull": "supabase db pull",
    "db:push": "supabase db push",
    "test": "vitest run"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.2.6",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "@vitejs/plugin-react": "^4.7.0",
    "dotenv": "^17.2.3",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "happy-dom": "^20.0.11",
    "prettier": "^3.2.5",
    "tsx": "^4.20.6",
    "typescript": "~5.9.3",
    "vite": "^7.2.4",
    "vitest": "^4.0.16"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.84.0",
    "lottie-react": "^2.4.1",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "zustand": "^5.0.8"
  }
}