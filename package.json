{
  "name": "gemtlemonster",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,css}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,json,css}\"",
    "seed:facilities": "tsx src/data/seedFacilities.ts",
    "seed:alchemy": "tsx src/data/seedAlchemy.ts",
    "seed:monsters": "tsx src/data/seedMonsters.ts",
    "seed:all": "npm run seed:facilities && npm run seed:alchemy && npm run seed:monsters",
    "types:generate": "supabase gen types typescript --project-id zcqejdvtwubaodfpjvbm --schema public > src/types/database.types.ts",
    "types:local": "supabase gen types typescript --local --schema public > src/types/database.types.ts",
    "db:pull": "supabase db pull",
    "db:push": "supabase db push"
  },
  "devDependencies": {
    "@types/react": "^19.2.6",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "@vitejs/plugin-react": "^4.7.0",
    "dotenv": "^17.2.3",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^3.2.5",
    "tsx": "^4.20.6",
    "typescript": "~5.9.3",
    "vite": "^7.2.4"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.84.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "zustand": "^5.0.8"
  }
}